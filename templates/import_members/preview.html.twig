{% extends '@EasyAdmin/page/content.html.twig' %}

{% block content_title %} {{ page_name|default('Imported file') }} {% endblock %}

{% block main %}

    <table class="table table-striped table-bordered table-hover">
        <thead>
            <tr>
            {% for column in data['columnNames'] %}
                <th scope="col">{{column}}</th>
            {% endfor %}
            </tr>
        </thead>
        <tbody>
            {% for row in data['columnValues'] %}
                <tr>
                {% for cell in row %}
                    <td>{{cell}}</td>
                {% endfor %}
                </tr>
            {% endfor %}
        </tbody>
    </table>

    <div>
        <a href='{{cancel_url}}' class='btn btn-outline-warning btn-lg'>
            Cancel
        </a>
        <a href='{{redirect_url}}' class='btn btn-primary btn-lg'>
            Submit
        </a>
    </div>
{% endblock %}

